  "scripts": {
    "build": "tsc",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts|json)\"",
    "create:env": "node ./scripts/create.env.js",
    "lint": "eslint --ignore-path .eslintignore --ext .ts --fix",
    "prebuild": "rimraf dist",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "prisma migrate dev",
    "prisma:migrate-reset": "prisma migrate reset",
    "start:dev": "pnpm prebuild && tsc -w & nodemon -q -r ./tsconfig-paths-bootstrap.js -w dist dist/src/main.js",
    "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/src/main.js",
    "build:prod": "prisma generate && tsc && prisma migrate deploy",
    "typecheck": "tsc --p ./tsconfig.json --noEmit"
  },
